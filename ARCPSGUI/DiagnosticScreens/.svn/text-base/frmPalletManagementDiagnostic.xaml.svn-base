<Window x:Class="ARCPSGUI.DiagnosticScreens.frmPalletManagementDiagnostic"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="frmPalletManagementDiagnostic" Height="650" Width="950"
        SnapsToDevicePixels="True" OverridesDefaultStyle="False"
        WindowStyle="SingleBorderWindow" ResizeMode="CanResize" WindowStartupLocation="CenterScreen"  
        SizeToContent="Manual" MouseLeftButtonDown="Window_MouseLeftButtonDown"
        ManipulationStarting="Window_ManipulationStarting" MouseDoubleClick="Window_MouseDoubleClick"
         Background="#FF070606">
    <Window.Resources>

        <ControlTemplate x:Key="roundedLabelTrue" TargetType="{x:Type Button}">

            <Border Name="Border" BorderBrush="#FF5EB12B" BorderThickness="1" 
                   CornerRadius="50" Padding="1" >
                <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFDEDACA" Offset="0" />
                        <GradientStop Color="#FFFFAE03" Offset="1" />
                        <GradientStop Color="#FFEEC466" Offset="0.261" />

                    </LinearGradientBrush>
                </Border.Background>

            </Border>

        </ControlTemplate>

        <ControlTemplate x:Key="maxButtonStyle" TargetType="{x:Type Button}">

            <Border Name="Border" BorderBrush="#FF5EB12B" BorderThickness="1" 
                   CornerRadius="50" Padding="1" >
                <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFDEDACA" Offset="0" />
                        <GradientStop Color="#FFECFF03" Offset="1" />
                        <GradientStop Color="#FFF5BE3C" Offset="0.261" />

                    </LinearGradientBrush>
                </Border.Background>

            </Border>

        </ControlTemplate>

        <ControlTemplate x:Key="closestyle" TargetType="{x:Type Button}">

            <Border Name="Border" BorderBrush="#FF5EB12B" BorderThickness="1" 
                   CornerRadius="50" Padding="1" >
                <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFDEDACA" Offset="0" />
                        <GradientStop Color="#FFFF3131" Offset="1" />
                        <GradientStop Color="#FFFF4B4B" Offset="0.261" />

                    </LinearGradientBrush>
                </Border.Background>

            </Border>

        </ControlTemplate>

        <ControlTemplate x:Key="roundedLabelTrue1" TargetType="{x:Type Label}">

            <Border Name="Border" BorderBrush="#FF5EB12B" BorderThickness="1" 
                   CornerRadius="50" Padding="1" >
                <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                <Border.Background>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#E913BE00" Offset="0.757" />
                        <GradientStop Color="#FFBAFFBA" Offset="1" />
                    </LinearGradientBrush>
                </Border.Background>

            </Border>

        </ControlTemplate>

        <Style x:Key="ResizeGripThumbStyle"
               TargetType="{x:Type Thumb}">
            <Setter Property="Stylus.IsPressAndHoldEnabled"
                        Value="False" />
            <Setter Property="Width"
                        Value="{DynamicResource {x:Static SystemParameters.VerticalScrollBarWidthKey}}" />
            <Setter Property="Height"
                        Value="{DynamicResource {x:Static SystemParameters.HorizontalScrollBarHeightKey}}" />
            <Setter Property="Background"
                        Value="#00FFFFFF" />
            <Setter Property="HorizontalAlignment"
                        Value="Right" />
            <Setter Property="VerticalAlignment"
                        Value="Bottom" />
            <Setter Property="Cursor"
                        Value="SizeNWSE" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Thumb}">
                        <Grid SnapsToDevicePixels="True"
                                       Background="{TemplateBinding Background}">
                            <Path HorizontalAlignment="Right"
                                             Margin="0,0,2,2"
                                             VerticalAlignment="Bottom"
                                             Data="M11,0L12,0 12,12 0,12 0,11z">
                                <Path.Fill>
                                    <DrawingBrush TileMode="Tile"
                                                                     Viewbox="0,0,4,4"
                                                                     ViewboxUnits="Absolute"
                                                                     Viewport="0,0,4,4"
                                                                     ViewportUnits="Absolute">
                                        <DrawingBrush.Drawing>
                                            <DrawingGroup>
                                                <GeometryDrawing Brush="#FFACB5BA"
                                                                                          Geometry="M0,0L2,0 2,1 1,1 1,2 0,2z M2,2L3,2 3,1 4,1 4,3 3,3 3,4 1,4 1,3 2,3z" />
                                                <GeometryDrawing Brush="#FFC3CED3"
                                                                                          Geometry="M0,3L1,3 1,4 0,4z M1,2L2,2 2,3 1,3z M2,1L3,1 3,2 2,2z M3,0L4,0 4,1 3,1z" />
                                            </DrawingGroup>
                                        </DrawingBrush.Drawing>
                                    </DrawingBrush>
                                </Path.Fill>
                            </Path>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    
    <Border BorderThickness="10" CornerRadius="5" >
        <Border.Effect>
            <DropShadowEffect Color="White"
                              Direction="270"
                              BlurRadius="10"
                              ShadowDepth="3" />
        </Border.Effect>

        <Grid Style="{StaticResource ContentGrid}" Background="#FF6F6B6B">
            <Grid.RowDefinitions>
            <RowDefinition Height="55" />
            <RowDefinition Height="328*" />
            <RowDefinition Height="15" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="221*" />
            <ColumnDefinition Width="324*" />
        </Grid.ColumnDefinitions>
        <Canvas Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="0" Style="{StaticResource ToolBarCanvas}" Visibility="Hidden">
            <Border>
                <Label Canvas.Left="1" Canvas.Top="-1" Content="PMS Diagnostic Screen" Name="label1" FontFamily="Verdana" BorderBrush="#FFC7A01A" Foreground="White">
                    <!--<Label.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFFFCB45" Offset="0" />
                            <GradientStop Color="#FFFFB90D" Offset="1" />
                            <GradientStop Color="#FFF5D38D" Offset="0.602" />
                            <GradientStop Color="#FFFFB20A" Offset="1" />
                            <GradientStop Color="#FFD8D0BF" Offset="0.117" />
                            <GradientStop Color="Orange" Offset="0" />
                            <GradientStop Color="#FFDACCB1" Offset="0.228" />
                        </LinearGradientBrush>
                    </Label.Background>-->
                </Label>
                <Border.Effect>
                    <DropShadowEffect Color="Black"
                              Direction="270"
                              BlurRadius="10"
                              ShadowDepth="3" />
                </Border.Effect>

            </Border>
        </Canvas>
        <Viewbox Height="21" Width="23"  Grid.Column="1" Margin="248,0,0,4" Style="{StaticResource ToolBarButtonViewBox}" HorizontalAlignment="Right">
            <Button Content="X" Template="{StaticResource CloseButtonTemplate}" Style="{StaticResource ToolBarButtonStyle}" HorizontalAlignment="Right" Click="btnClose_Click" Visibility="Hidden"></Button>
        </Viewbox>
        <Viewbox Height="21" Width="23" Grid.Column="1" Margin="222,0,26,4" Style="{StaticResource ToolBarButtonViewBox}">
            <Button Content="+" Template="{StaticResource MaxButtonTemplate}" Style="{StaticResource ToolBarButtonStyle}" 
                    Click="btnMaximize_Click" IsEnabled="True" Visibility="Hidden"></Button>
        </Viewbox>
        <Viewbox Height="21" Width="23"  Grid.Column="1" Margin="197,0,51,4" Style="{StaticResource ToolBarButtonViewBox}">
            <Button Content="-" Template="{StaticResource MinimizeButtonTemplate}" Style="{StaticResource ToolBarButtonStyle}"  Click="btnMinimize_Click" Visibility="Hidden"></Button>
        </Viewbox>
                
                <Grid Name="grdMachineHolder" Height="Auto"  Width="Auto" Style="{StaticResource ContentGrid}" Grid.Row="1"
              Grid.Column="0" Grid.ColumnSpan="2" Background="#FF6F6B6B">
            <Grid.RowDefinitions>
                <!--<RowDefinition />
            <RowDefinition Height="5" />
            <RowDefinition />
            <RowDefinition Height="5" />
            <RowDefinition />-->
                <RowDefinition Height="5" />
                <RowDefinition Height="60*"/>
                <!--80*-->
                <RowDefinition Height="10" />
                <!--<RowDefinition Height="25*" />
            <RowDefinition Height="30*" />-->
                <RowDefinition Height="15*" />
                <RowDefinition Height="30*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="5" />
                <!--floorno-->
                <ColumnDefinition Width="3" />
                <ColumnDefinition Width="50*" />
                <!--lcm1-->
                <ColumnDefinition Width="5*" />
                <ColumnDefinition Width="50*" />
                <!--lcm2-->
                <ColumnDefinition Width="100*" />
                <ColumnDefinition Width="50*" />
                <!--lcm3-->
                <ColumnDefinition Width="5*" />
                <ColumnDefinition Width="50*" />
                <ColumnDefinition Width="5*" />
                <ColumnDefinition Width="25*" />
                <ColumnDefinition Width="5*" />
                <ColumnDefinition Width="25*" />
                <ColumnDefinition Width="5" />
            </Grid.ColumnDefinitions>
            <Grid Name="grdMachineHolder2" Grid.Column="0" Grid.ColumnSpan="13" Grid.Row="0" Grid.RowSpan="2"  HorizontalAlignment="Stretch" 
              VerticalAlignment="Stretch" Width="Auto" >
                <Grid.RowDefinitions>
                    <RowDefinition Height="20"/>
                    <RowDefinition Height="19*"/>
                    <RowDefinition Height="15*"/>
                    <RowDefinition  Height="5"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                    <Border BorderBrush="#FFFFFCFC" BorderThickness="2" Grid.Column="3" Grid.Row="1" Grid.RowSpan="2" HorizontalAlignment="Stretch" Name="border1" VerticalAlignment="Stretch" />
                    <Border BorderBrush="White" BorderThickness="0.5" Grid.Column="4" Grid.Row="1" Grid.RowSpan="2" HorizontalAlignment="Stretch" Name="border2" VerticalAlignment="Stretch" />
                    <Border BorderBrush="White" BorderThickness="0.5" Grid.Column="12" Grid.Row="1" Grid.RowSpan="2" HorizontalAlignment="Stretch" Name="border3" VerticalAlignment="Stretch" />
                </Grid>
            
            </Grid>
            
            
            <Label Content="PMS Diagnostic Screen" Grid.ColumnSpan="2" HorizontalAlignment="Stretch"  Name="label2" VerticalAlignment="Stretch"  FontFamily="Tahoma" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="28" Background="#FF969696" Margin="2,3,4,3" Foreground="White" />
          
        </Grid>
    </Border>
</Window>
