<Window x:Class="ARCPSGUI.Popup.PhotoPop"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Photo" Height="300" Width="500" Topmost="True" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" >
    <Viewbox Stretch="Fill">
        <Grid Background="#FFB2B2B2" Height="270" Width="492">
            <Image x:Name="NortImg" HorizontalAlignment="Left" Height="204" Margin="10,56,0,0" VerticalAlignment="Top" Width="215"/>
            <Image x:Name="SouthImg" HorizontalAlignment="Left" Height="204" Margin="254,56,0,0" VerticalAlignment="Top" Width="215"/>
            <TextBlock x:Name="eesName" HorizontalAlignment="Left" Margin="230,10,0,0" TextWrapping="Wrap" Text="EES1" VerticalAlignment="Top" Foreground="#FFAA1212" FontWeight="Bold"/>
            <TextBlock HorizontalAlignment="Left" Margin="94,28,0,0" TextWrapping="Wrap" Text="North" VerticalAlignment="Top" Foreground="#FFAA1212" FontWeight="Bold"/>
            <TextBlock HorizontalAlignment="Left" Margin="344,28,0,0" TextWrapping="Wrap" Text="South" VerticalAlignment="Top" Foreground="#FFAA1212" FontWeight="Bold"/>

        </Grid>
    </Viewbox>
</Window>
