<Window x:Class="ARCPSGUI.utility.winRangeSlider"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:avalon="http://schemas.AvalonControls/AvalonControlsLibrary/Controls"
        Title="winRangeSlider" Height="300" Width="300" x:Name="root">
    <Grid>

        <avalon:RangeSlider x:Name="rangeSlider"  RangeStart="0" RangeStop="100"
                    RangeSelectionChanged="RangeSlider_RangeSelectionChanged" Height="20" />
        <Label x:Name="min" Content="" HorizontalAlignment="Left" Margin="37,89,0,0" VerticalAlignment="Top"/>
        <Label x:Name="max" Content="" HorizontalAlignment="Left" Margin="216,94,0,0" VerticalAlignment="Top"/>


    </Grid>
</Window>
